            <string>///Main menu
    //scene management
        /*
            notes:
            m_menu_scene will determine where the over will be displayed
            rm_titlescreen will be decorated with different area's and a timer will
            be set to cycle through these area's
        */
            if (room = rm_titlescreen)
            {
                clock = true
                if (m_scene = 0)
                {
                    x += .01
                    if (time &gt;= 240)
                    {
                        m_scene_fade = 1
                    }
                    if (m_scene_alpha &gt;= 1)
                    {
                        x = 0
                        m_scene = 0
                        time = 0
                        m_scene_fade = -1
                    }
                }
            }
    //section selection's
        //no selection
            if (room = rm_titlescreen)
            {
                if (!m_newgame &amp;&amp; !m_continue &amp;&amp; !m_option &amp;&amp; !m_credit)
                {
                    if (m_confirm_lock = 0)
                    {
                        //quit
                            if (line = 4)
                            {
                                m_menuselecty = 45
                                if (key_confirm)
                                {
                                    game_end()
                                }
                            }
                        //credits
                            if (line = 3)
                            {
                                m_menuselecty = 35
                            }
                        //options 
                            if (line = 2)
                            {
                                m_menuselecty = 25
                            }
                        //continue
                            /*
                                note: if save file exists, player will be able to access this, otherwise they will not
                            */
                            if (line = 1)
                            {
                                m_menuselecty = 15
                            }
                        //new game
                            if (line = 0)
                            {
                                m_menuselecty = 5
                                if (key_confirm)
                                {
                                    global.fade = 1
                                    if (!instance_exists(obj_fade))
                                    {
                                        instance_create(1,1,obj_fade)
                                    }
                                    m_confirm_lock = 1
                                }
                            }
                    }
                }
        //line setup's
            if (!m_newgame &amp;&amp; !m_continue &amp;&amp; !m_option &amp;&amp; !m_credit)
            {
                if (m_confirm_lock = 0)
                {
                    if (key_up)
                    {
                        if (line - 1 &gt;= 0)
                        {
                            line -= 1
                            //play sound
                        }
                    }
                    if (key_down)
                    {
                        if (line + 1 &lt;= 4)
                        {
                            line += 1 
                            //play sound
                        }
                    }
                }
            }
            
        //confirm reset
            if (m_confirm_lock = 1)
            {
                m_confirm_lock = 0
            }
        }
</string>
